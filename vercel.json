{
  "version": 2,
  "cleanUrls": true,
   "functions": {
    "static/**": {
      "maxDuration": 1024 
    }
  },
  "builds": [
    { "src": "*.vtt", "use": "@vercel/static" },
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.js"
    },
    {
      "src": "/:lec",
      "dest": "index.js"
    },
    {
      "src": "/static/:lec",
      "dest": "/static/$lec"
    }
  ]
}
