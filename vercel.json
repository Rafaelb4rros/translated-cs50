{
  "builds": [
    { "src": "*.vtt", "use": "@vercel/static" },
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
       "src": "/static/(.*)",
       "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31556952, immutable"
        }
    },
    {
      "src": "/:lec",
      "dest": "index.js"
    },
    {
      "src": "/static/:lec",
      "dest": "/static/:lec"
    }
  ]
}
