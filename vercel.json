{
  "builds": [
        { "src": "/static/*.vtt", "use": "@vercel/static" },
        {
          "src": "index.js",
          "use": "@now/node"
        }
    ],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "s-maxage=2592000, stale-while-revalidate=59"
            }
          ]
        }
    ],
    "routes": [
        {
            "src": "/static/(?<id>[^/]*)",
            "dest": "/static/$id"
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}
