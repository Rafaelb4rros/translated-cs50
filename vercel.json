{
  "builds": [
        { "src": "/static/*.vtt", "use": "@vercel/static" },
        {
          "src": "index.js",
          "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "index.js"
        },
        {
            "src": "/(?<id>[^/]*)/",
            "dest": "index.js"
        },
        {
            "src": "/static/(?<id>[^/]*)",
            "dest": "/static/$id"
        }
    ]
}
