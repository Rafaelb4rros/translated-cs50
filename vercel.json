{
  "builds": [
        { "src": "/static/*.vtt", "use": "@vercel/static" },
        {
          "src": "index.js",
          "use": "@now/node"
        }
    ],
    "routes": [
        {
           "src": "/static/(.*)",
           "headers": [
                {
                  "key": "Cache-Control",
                  "value": "public, max-age=31556952, immutable"
                }
           ]
        },
        {
            "src": "/",
            "dest": "index.js"
        },
        {
            "src": "/(?<id>[^/])",
            "dest": "index.js"
        },
        {
            "src": "/static/(?<id>[^/]*)",
            "dest": "/static/$id"
        }
    ]
}
